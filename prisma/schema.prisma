

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}



model Budgets {
  id        Int      @id @default(autoincrement())
  name      String 
  amount    String
  icon      String
  createdBy String
  expenses  Expenses[]
}



model Expenses {
  id        Int      @id @default(autoincrement())
  name      String
  amount    Float @default(0)
  budgetId  Int
  Budgets    Budgets  @relation(fields: [budgetId], references: [id])
  createdAt String
}